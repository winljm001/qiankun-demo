{
  "name": "pitaya-web",
  "version": "1.6.0",
  "scripts": {
    "dev": "vite",
    "build:test": "vite build --mode test",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint src --ext .ts,.tsx",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx",
    "prettier": "prettier --write 'src/**/*.{js,json,yml,yaml,ts,tsx,md}'",
    "ncu": "npx npm-check-updates -u -x zustand,husky",
    "prepare": "husky install",
    "resolutions": "npx npm-force-resolutions",
    "lint-staged": "lint-staged --allow-empty",
    "lint:gql:generated": "eslint src/graphql/operations --ext .ts --fix",
    "prettier:gql:generated": "prettier --write 'src/graphql/operations/**/*.ts'",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.8",
    "@ant-design/icons": "^4.6.4",
    "@apollo/client": "^3.4.10",
    "@loadable/component": "^5.15.0",
    "ahooks": "^2.10.9",
    "antd": "^4.16.13",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^15.5.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.1",
    "shallowequal": "^1.1.0",
    "wbd-frontend-kit": "^1.0.13",
    "zustand": "3.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@fruits-chain/eslint-config-preset": "^0.0.4",
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/near-operation-file-preset": "^2.1.2",
    "@graphql-codegen/schema-ast": "^2.1.0",
    "@graphql-codegen/typescript": "^2.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@graphql-codegen/typescript-react-apollo": "^3.1.2",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "apollo": "^2.33.6",
    "husky": "6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "pont-engine": "1.0.13",
    "postcss": "^8.3.6",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-style-import": "^1.2.1"
  },
  "resolutions": {
    "prettier": "^2.3.1"
  },
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
